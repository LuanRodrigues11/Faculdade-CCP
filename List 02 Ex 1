//Faça	um programa que imprima na tela os números de 1 a 20, um abaixo do	outro.
//Aluno: Luan Rodrigues
//Instituição: If Sudeste MG - Rp
//Período: Primeiro Período


#include<iostream>
using namespace std;


int main(){
	int i = 1;
	float b = 0;
	
	while(i<=20){
		b = b + 1;
		cout<<b<<endl;
		i++;
	}
	return 0;
}
